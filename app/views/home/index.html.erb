         <!-- CONTACT -->
         <div id="contact">
            <div class="line">
               <h2 class="section-title">Contact Me</h2>
               <div class="margin">
                  <div class="s-12 m-12 l-3 hide-m hide-s margin-bottom right-align">
                    <%= image_tag("contact.jpg") %>
                  </div>
                  <div class="s-12 m-12 l-4 margin-bottom right-align">
                     <h3>Vision Design - graphic zoo</h3>
                     <address>
                        <p><strong>Adress:</strong> Ventura, California</p>
                        <p><strong>Country:</strong> U.S.</p>
                        <p><strong>E-mail:</strong> liwenc12@gmail.com</p>
                     </address>
                  </div>
                  <div class="s-12 m-12 l-5">
                    <h3>Example contact form (do not use)</h3>
                    <%= form_tag("/emails", method: "post" , action:"create", class: "customform") do %>
                      <div class="s-12">
                        <%= label_tag(:email, "Your Email:") %>
                        <%= text_field_tag(:email) %>
                      </div>
                      <div class="s-12">
                        <%= label_tag(:name, "Your Name:") %>
                        <%= text_field_tag(:name) %>
                      </div>
                      <div class="s-12">
                        <%= label_tag(:Message, "Message:") %>
                        <%= text_area_tag(:message) %>
                      </div>
                      <div class="s-12 m-12 l-4">
                        <%= submit_tag("Submit", :class => 'color-btn') %>
                      </div>
                    <% end %>
                      <div class="s-12">
                        <% @list_contacts.each do |list_contact| %>
                        <ul class="list-contact">
                          <li><%= list_contact.email %></li>
                          <li><%= list_contact.name %></li>
                          <li><%= list_contact.message %></li>
                          <li><%= link_to 'Delete', email_path(list_contact.id), action: "delete", data:{confirm: 'Are you certain???'}%></li>
                        </ul>
                        <% end %>
                      </div>
                    </div>
               </div>
            </div>
         </div>
